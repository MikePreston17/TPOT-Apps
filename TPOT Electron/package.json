{
    "name": "TPOT_Toolbox",
    "description": "A collection of utilities for ThePathofTruth members",
    "version": "0.0.6",
    "author": "Michael & Braden Preston",
    "license": "MIT",
    "private": true,
    "homepage": "./",
    "repository": "https://github.com/MikePreston17/TPOTDev",
    "main": "public/electron.js",
    "scripts": {
        "test": "mocha",
        "main": "electron .",
        "react-start": "react-scripts start",
        "react-build": "react-scripts build",
        "react-test": "react-scripts test --env=jsdom",
        "react-eject": "react-scripts eject",
        "electron-build": "electron-builder",
        "release": "yarn react-build && electron-builder --publish=always",
        "bundle": "yarn react-build",
        "package": "yarn electron-build",
        "build": "yarn bundle && yarn package",
        "ship": "yarn build -p always",
        "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\""
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
        "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
        "@fortawesome/react-fontawesome": "0.1.0-11",
        "@jimmycode/draft-js-soft-newline-plugin": "^0.5.1",
        "@material-ui/core": "^1.4.2",
        "@material-ui/icons": "^2.0.0",
        "@material-ui/lab": "^1.0.0-alpha.9",
        "babel": "^6.23.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.5",
        "builtin-modules": "^3.0.0",
        "chai": "^4.1.2",
        "chalk": "^2.4.1",
        "computed-style-to-inline-style": "^3.0.0",
        "computed-styles": "^1.1.2",
        "create-node": "^2.0.3",
        "cross-env": "5.1.6",
        "css": "^2.2.4",
        "css-object-loader": "0.0.7",
        "css-to-js-object": "0.0.3",
        "devtron": "^1.4.0",
        "domwalk": "^1.1.0",
        "draft-convert": "^2.1.4",
        "draft-js": "^0.10.5",
        "draft-js-alignment-plugin": "^2.0.3",
        "draft-js-anchor-plugin": "^2.0.2",
        "draft-js-custom-styles": "^2.0.4",
        "draft-js-export-html": "^1.2.0",
        "draft-js-focus-plugin": "^2.2.0",
        "draft-js-image-plugin": "^2.0.6",
        "draft-js-import-element": "^1.2.1",
        "draft-js-inline-toolbar-plugin": "^2.0.3",
        "draft-js-link-plugin": "^1.2.2",
        "draft-js-plugins-editor": "^2.1.1",
        "draft-js-side-toolbar-plugin": "^3.0.0",
        "draft-js-static-toolbar-plugin": "^2.0.2",
        "draftjs-utils": "^0.9.4",
        "electron-debug": "^2.0.0",
        "electron-is-dev": "0.3.0",
        "electron-log": "^2.2.6",
        "electron-react-devtools": "^0.5.3",
        "electron-updater": "^3.1.2",
        "file2html": "^0.1.9",
        "file2html-image": "^0.1.1",
        "file2html-ooxml": "^0.2.2",
        "file2html-text": "^0.2.0",
        "firebase": "^5.4.2",
        "firebase-admin": "^6.0.0",
        "firebaseui": "^3.4.0",
        "googleapis": "^27.0.0",
        "html-beautify": "^1.0.4",
        "html-to-draftjs": "^1.4.0",
        "immutable": "^3.8.2",
        "jquery": "^3.3.1",
        "last-draft-js-toolbar-plugin": "^1.2.0",
        "mammoth": "1.4.6",
        "mammoth-colors": "1.0.2",
        "mdi-material-ui": "^5.2.0",
        "mobx": "^5.5.0",
        "mobx-react": "^5.2.8",
        "promise-fs": "^1.3.0",
        "random-number": "0.0.9",
        "randomcolor": "^0.5.3",
        "react": "16.4.0",
        "react-dom": "16.4.0",
        "react-fontawesome": "^1.6.1",
        "react-frame-component": "^4.0.1",
        "react-html-parser": "^2.0.2",
        "react-json-pretty": "^1.7.9",
        "snake-case": "^2.1.0",
        "to-virtual-dom": "^1.0.0",
        "typeface-roboto": "0.0.54",
        "underscore": "^1.9.1",
        "update-electron-app": "^1.2.0",
        "wordpress": "^1.4.1",
        "wpapi": "^1.1.2"
    },
    "devDependencies": {
        "babel-preset-mobx": "^2.0.0",
        "concurrently": "3.5.1",
        "electron": "^2.0.8",
        "electron-builder": "^20.15.1",
        "mocha": "^5.2.0",
        "prettier": "^1.4.4",
        "react-scripts": "1.1.4",
        "wait-on": "2.1.0"
    },
    "babel": {
        "plugins": [
            "transform-decorators-legacy"
        ],
        "presets": [
            "react-app"
        ]
    },
    "build": {
        "appId": "tpot.toolbox.app",
        "productName": "TPOT Toolbox",
        "copyright": "CopyrightÂ© 2018 Michael & Braden Preston",
        "icon": "public/defaultIcon.ico",
        "files": [
            "**/*",
            "public/*ico"
        ],
        "extraFiles": [
            {
                "from": "src/config",
                "to": "storage/drafts",
                "filter": [
                    "**/*"
                ]
            },
            {
                "from": "src/config",
                "to": "storage/users",
                "filter": [
                    "**/*",
                    "!**/foo/*.ico"
                ]
            }
        ],
        "win": {
            "icon": "public/defaultIcon.ico"
        },
        "nsis": {
            "oneClick": true,
            "perMachine": true,
            "allowElevation": false,
            "allowToChangeInstallationDirectory": false,
            "deleteAppDataOnUninstall": false,
            "differentialPackage": true,
            "createStartMenuShortcut": true,
            "createDesktopShortcut": "always",
            "menuCategory": "TPOT",
            "installerIcon": "public/installerIcon.ico",
            "uninstallerIcon": "public/uninstallerIcon.ico",
            "installerHeader": "public/installerHeader.bmp",
            "installerHeaderIcon": "public/defaultIcon.ico",
            "installerSidebar": "public/installerSidebar.bmp",
            "uninstallerSidebar": "public/uninstallerSidebar.bmp"
        }
    }
}
